<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="view_company_chronopost_form">
        <field name="name">res.company.form.chronopost</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="model">res.company</field>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Chronopost Info">
                    <field name="chronopost_account_ids"/>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
