<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_picking_view_form_inherit_stock_barcode" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_generate_carrier_label']" position="after">
                    <button name="print_delivery" string="Imprimer Etiquette(s)" custom="print_label" groups="stock.group_stock_user" type="object" states="done" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>